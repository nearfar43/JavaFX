            <TextField fx:id="trialText" layoutX="251.0" layoutY="200.0" prefHeight="25.0" prefWidth="103.0" />
            <Label layoutX="189.0" layoutY="204.0" text="Trial File:" />
            <Button fx:id="secnarioButton" layoutX="388.0" layoutY="141.0" mnemonicParsing="false" onAction="#chooseScenarioFile" text=",,," />
